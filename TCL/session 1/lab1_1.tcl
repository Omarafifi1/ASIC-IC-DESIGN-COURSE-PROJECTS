puts "aithmetic operations on integer operands"
set vara 35
set varb 80
set var00 [expr $vara + $varb ]
set var01 [expr $vara - $varb ]
set var02 [expr $vara * $varb ]
set var03 [expr $vara / $varb ]
set var04 [expr $vara % $varb ]
puts "a + b = $var00"
puts "a - b = $var01"
puts "a * b = $var02"
puts "a / b = $var03"
puts "a % b = $var04"

puts "aithmetic operations on floating operands"
set vara 35.0
set varb 80
set var00 [expr $vara + $varb ]
set var01 [expr $vara - $varb ]
set var02 [expr $vara * $varb ]
set var03 [expr $vara / $varb ]
set var04 [expr $vara % $varb ]
puts "a + b = $var00"
puts "a - b = $var01"
puts "a * b = $var02"
puts "a / b = $var03"
puts "a % b = $var04"

